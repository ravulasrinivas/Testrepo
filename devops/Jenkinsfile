pipeline
{
  agent any
  stages{
    
    stage('gitpullstage'){
    steps{      
            git branch: 'branch-team', credentialsId: 'sreenivas', url: 'https://github.com/ravulasrinivas/company-name.git'
          }
    
    }
    stage('buildeploy'){
    steps{   
          git update-index --chmod=+x path/to/deploy.sh
          sh "./deploy.sh"
           }
     }
    
  } 
  
}

