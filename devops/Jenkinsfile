pipeline
{
  agent any
  stages{
    
    stage('gitpullstage'){
    steps{      
            git branch: 'branch-team', credentialsId: 'sreenivas', url: 'https://github.com/ravulasrinivas/company-name.git'
          }
    
    }
    stage('buildeploy'){
    steps{  
           cd /var/lib/jenkins/workspace/git-demo
           chmod 777 deploy.sh
           sh "./deploy.sh"
           }
     }
    
  } 
  
}

